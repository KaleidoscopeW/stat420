make_decision <- function(pval, alpha = 0.05){
if (pval < alpha/2){
return("Fail to Reject")
}
return("Reject")
}
set.seed(114)
y = rnorm(25, 1.4, 1)
pval = do_t_test(y, mu = 2)[2]
do_t_test <- function(x, mu = 0){
n <- length(x)
x_bar <- sum(x)/n
t <- (x_bar-mu)/(sd(x)/sqrt(n))
pv <- 2*pt(-abs(t),df=n-1)
return(c(t, pv))
}
set.seed(114)
y = rnorm(25, 1.4, 1)
pval = do_t_test(y, mu = 2)[2]
pval
make_decision(pval, alpha = 0.10)
